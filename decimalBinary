import java.util.*;
public class Main {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		System.out.print("Enter a decimal: ");
		int n = s.nextInt();
		int rem;
		String result = " ";
		while(n != 0){
		rem = n%2;
		result = result + rem;
		n = n/2;	
		}
		System.out.print(result);
	}
}