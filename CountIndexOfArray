import java.util.*;
public class Main {
	public static void main(String[] args) {
		int[] arr = {3, 12, 4, 9, 2, 9, 10, 45, 72, 12, 87, 8, 2, 4, 19, 26, 7, 24, 16, 11, 28, 24, 18, 12, 16, 20, 18};
		
		int count = 0;
		for(int i = 0; i <= arr.length-1;i++){
			  count++;
		}
		System.out.println("The index of array is " + count);
	}
}