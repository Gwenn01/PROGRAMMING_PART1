import java.util.*;
public class Main {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		
		System.out.print("Enter a Even Numbers: ");
		int num = s.nextInt();
		int result = 2;
		System.out.println();
		System.out.println("2");	
		
		for(int i = 2; i <= num; i++){
			result = result + 2;
			System.out.println(result);
		}
		
				
	}
}