import java.util.*;
public class Main {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		System.out.print("Enter a digits to be count: ");
		int n = s.nextInt();
		
		int rem, count = 0;
		
		while(n != 0){
			count++;
			n/= 10;
		}
		System.out.println("The total digits is " + count);
		
	}
}