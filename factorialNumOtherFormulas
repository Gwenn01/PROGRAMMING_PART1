import java.util.*;
public class Main {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		System.out.print("Enter a number: ");
		int n = s.nextInt();
		int result = n;
		while(n != 1){
		n--;	
		result = result * n;	
		}
		System.out.print("The factorial is " + result);
	}
}