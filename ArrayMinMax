public class Main {
	public static void main(String[] args) {
		
		int[] arr = {3, 4, 5, 2, 7, 9, 5, -3};
		for(int i = 0; i <= arr.length-1; i++){
			System.out.print(arr[i] + " ");
		}
		
		int min = 100, max = 0;
		
		for(int i = 0; i <= arr.length-1; i++){
		      if(arr[i] <= min)	
			     min = arr[i];
			     
			  if(arr[i] >= max)
			     max = arr[i];
		}
		System.out.println();
		System.out.print("min = " + min + " max = " + max);
	}
}