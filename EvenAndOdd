public class Main {
	public static void main(String[] args) {
		int n = 100;
		int rem;
		System.out.println("Even numbers");
		for(int i = 0; i <= n; i++){
		if(i%2 == 0){
			System.out.println(i);
			}	
		}
		System.out.println("Odd numbers");
		for(int j = 0; j <= n; j++){
		if(j%2 == 1){
			System.out.println(j);
			}	
		}
		
		
	}
}