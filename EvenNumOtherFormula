import java.util.*;
public class Main {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		
		System.out.print("Enter a Odd Numbers: ");
		int num = s.nextInt();
		System.out.println();
		
		for(int i = 1; i <= num; i++){
		 if(i % 2 == 0){
		 	System.out.println(i);
		 		
		 }	
			
		}
		
		
	}
}