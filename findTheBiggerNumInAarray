import java.util.*;
public class Main {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int[] arr = {1, 5, 6, 2, 3};
		int larger = 0;
		for(int i = 0; i <= arr.length -1; i++){
			System.out.print(arr[i] + " ");
			if(larger < arr[i]){
			larger = arr[i];
			}
		}
	System.out.println();
	System.out.println("The larger number in array is " + larger);	
		
	}
}