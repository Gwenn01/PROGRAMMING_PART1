import java.util.*;
public class Main {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		
		System.out.print("Enter the term: ");
		int n = s.nextInt();
		int result = 2;
		System.out.println("2");
		for(int i = 1; i <= n; i++){
			if(result == i){
			result = result * 3;
			System.out.println(result);
			}
		}
	
	}
}