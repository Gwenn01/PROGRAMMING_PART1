import java.util.*;
public class Main {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		System.out.print("Input an integer: ");
		int num = s.nextInt();
		
		int factors = 0;
		
		for(int i = 1; i <= num; i++){
			if(num%i == 0)
			factors++;
		}
		System.out.print(factors);
	}
}